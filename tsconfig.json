{
    "compilerOptions": {
      "strictNullChecks": true,
      "moduleResolution": "node",
      "esModuleInterop": true,
      "experimentalDecorators": true,
      "jsx": "preserve",
      "noUnusedParameters": false,
      "noUnusedLocals": true,
      "noImplicitAny": false,
      "target": "es6",
      "lib": ["dom", "es7"]
    },
    "exclude": ["node_modules", "lib", "es"]
  }
  